{
  "name": "chrome-cdp-cli",
  "version": "1.0.2",
  "description": "Command-line tool for controlling Chrome browser instances via the Chrome DevTools Protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "chrome-cdp-cli": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "node scripts/build.js development",
    "build:prod": "node scripts/build.js production",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
    "demo": "ts-node demo.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "package": "npm run build:prod && npm pack",
    "prepublishOnly": "npm run test:ci && npm run build:prod",
    "prepack": "npm run build:prod",
    "postinstall": "npm run build",
    "verify": "npm run lint && npm run test:ci && npm run build:prod"
  },
  "keywords": [
    "chrome",
    "devtools",
    "automation",
    "cli",
    "browser",
    "testing",
    "cdp",
    "chrome-devtools-protocol",
    "browser-automation",
    "headless-chrome"
  ],
  "author": "Chrome DevTools CLI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicoster/chrome-devtools-cli.git"
  },
  "bugs": {
    "url": "https://github.com/nicoster/chrome-devtools-cli/issues"
  },
  "homepage": "https://github.com/nicoster/chrome-devtools-cli#readme",
  "dependencies": {
    "commander": "^11.1.0",
    "node-fetch": "^2.7.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/node-fetch": "^2.6.13",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
